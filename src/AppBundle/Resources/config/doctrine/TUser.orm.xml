<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
<!-- repository-class="AppBundle\Repository\UserRepository"-->
  <entity name="AppBundle\Entity\TUser" table="t_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthDate" type="date" column="birth_date" nullable="false"/>
    <field name="createTime" type="datetime" column="create_time" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="address" type="string" column="address" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
