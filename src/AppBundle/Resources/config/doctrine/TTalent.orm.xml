<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TTalent" table="t_talent">
    <indexes>
      <index name="fk_id_user_idx" columns="id_user"/>
    </indexes>
    <id name="id" type="integer" column="id"/>
    <id name="idUser" association-key="true"/>
    <field name="description" type="string" column="description" length="180" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="idUser" target-entity="TUser" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
